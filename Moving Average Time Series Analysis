    "# full performance of MA1 on all data\n",
    "id = df.index\n",
    "T = len(df)\n",
    "y = df.to_frame(name = \"Sales\")\n",
    "y['y_hat'] = 0\n",
    "\n",
    "for t in range(2,T):\n",
    "    y_temp = df.iloc[:t]\n",
    "    y_temp = y_temp.asfreq(\"D\")\n",
    "    model = sm.tsa.ARIMA(y_temp, order=(0,0,1)).fit()\n",
    "    yh = model.get_forecast(steps=1)\n",
    "    y.at[y.index[t], 'y_hat'] = yh.predicted_mean.values[0]"
   ]
  },
  {
