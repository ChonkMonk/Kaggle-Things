    "# building the MA(1) Model Manually\n",
    "# TODO: FINISH THIS AFTER MIDTERMS\n",
    "def mu(df, column, t):\n",
    "    return df[column][:t].mean\n",
    "\n",
    "def f_t1(theta, mu, e_t):\n",
    "    # forecasts one period into future\n",
    "    return mu + e_t"
   ]
