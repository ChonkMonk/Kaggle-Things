   "outputs": [],
   "source": [
    "# manual programming of ACF and PACF\n",
    "def acf(theta, order, model_order):\n",
    "    if order <= model_order:\n",
    "        rho = theta / (1 + theta ** 2)\n",
    "    else:\n",
    "        rho = 0\n",
    "    return rho\n",
    "\n",
    "\n",
    "def pacf(theta, order):\n",
    "    r = ((- theta) ** order) * (1 - theta ** 2) / (1 - theta ** (2 * (order + 1)))\n",
    "    return r"
   ]
  },
